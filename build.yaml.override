apiVersion: "v1"
kind: "BuildConfig"
metadata:
  name: "echo-server-test"
spec:
  source:
    type: "Dockerfile"
  strategy:
    type: "Docker"
    dockerStrategy:
      dockerfilePath: docker/nginx-custom
      noCache: true
  output:
    to:
      kind: "ImageStreamTag"
      name: "echo-server-test:latest"

